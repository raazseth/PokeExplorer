{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["Components/*"],
      "@screens/*": ["Screen/*"],
      "@assets/*": ["Assets/*"],
      "@utils/*": ["Utils/*"],
      "@template/*": ["Template/*"],
      "@core/*": ["Components/Core/*"],
      "@services/*": ["Services/*"],
      "@navigation/*": ["Navigation/*"],
      "@typed/*": ["Types/*"],
      "@hooks/*": ["Hooks/*"],
      "@redux/*": ["Redux/*"],
      "@config/*": ["Config/*"]
    }
  }
}
